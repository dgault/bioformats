<?xml version="1.0" encoding="UTF-8"?>
<configuration>
  <appender name="stdout" class="ch.qos.logback.core.ConsoleAppender">
    <encoder>
      <pattern>%m%n</pattern>
    </encoder>
  </appender>
<logger name="loci.formats.FormatHandler" level="ERROR" />

  <root level="INFO">
    <appender-ref ref="stdout"/>
  </root>
</configuration>
